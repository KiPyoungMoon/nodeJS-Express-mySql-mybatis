{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "import/extensions": [
            "off"
        ],
        "prefer-const": [
            "off"
        ],
        "import/no-mutable-exports": [
            "off"
        ]
        
    }
}
